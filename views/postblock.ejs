<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.5.16/vue.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.css"/>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.js"></script>

</head>
<body>
    <template id="vue-temp">
        <div class="ui card">
            <div class="ui container" id="data-display"></div>
            <div class="content">
                <a class="header">{{ title }}</a>
                <div class="meta">
                    <span class="date">{{ posttime }}</span>
                </div>
                <div class="description">{{ description }}</div>
            </div>
            <div class="extra content">
                <script></script>
                <div class="ui selection dropdown">
                    <input type="hidden" name="responses">
                    <i class="dropdown icon"></i>
                    <div class="default text">temp</div>
                    <div class="menu">
                        <div class="item" data-value="1">Male</div>
                        <div class="item" data-value="0">Female</div>
                        <div class="item" v-for="item in items">{{ item.text }}</div>
                    </div>
                </div>
            </div>

        </div>
    </template>

    <div id="vue-app">
        <vue-post></vue-post>
        <vue-post></vue-post>
        <vue-post></vue-post>
        <vue-post></vue-post>
    </div>
    <script>
        $(document).ready(function(){
            $('.ui.dropdown').dropdown();
        });
    </script>
    <script src="/javascripts/Postvue.js"></script>
</body>
</html>